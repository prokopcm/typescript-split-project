{
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "module": "commonjs",
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true,
    "strictNullChecks": true,
  }
}
